<div class="page-header">
  <h1>Interaction #<%= @interaction.id %></h1>
</div>

<dl class="dl-horizontal">
  <dt>User</dt>
  <dd><%= @interaction.user.email %></dd>

  <dt>Contact</dt>
  <dd><%= @interaction.contact.name %></dd>

  <dt>Date</dt>
  <dd><%= @interaction.date %></dd>

  <dt>Message</dt>
  <dd><%= @interaction.message %></dd>

  <dt>Lastremindedon</dt>
  <dd><%= @interaction.lastremindedon %></dd>

</dl>
